{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "data/**",
          "lib/**",
          "routes/**",
          "scripts/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  }
}